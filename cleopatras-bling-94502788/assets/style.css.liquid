/* Contain floats: nicolasgallagher.com/micro-clearfix-hack/ */
.clearfix:before.centered,.clearfix:after {
    content: "";
    display: table;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    zoom: 1;
}

/**** theme.liquid ****/
.container {
    width: 100%;
    margin: 0 auto;
}

@media (min-width: 1200px) {
    .container {
        max-width: 1200px;
        margin: 0 auto;
    }
}

.header-toolbar {
    font-size: 12px;
    padding: 0;
    border-bottom: 1px solid #ccc;
}

.store-title img {
    display: block;
    margin: 0 auto;
}

.main-menu {
    text-align: center;
    border-top: 1px solid #ccc;
    border-bottom: 1px solid #ccc;
}

.main-menu li {
    display: inline-block;
    padding: 0 20px;
}

.main-menu a {
    color: #000;
    font-weight: 700;
}

nav.topNav {
    position: relative;
    z-index: 1000;
    display: block;
    border-top: 1px solid #ccc;
}

nav.topNav ul {
    list-style: none;
    list-style-image: none;
    margin: 0;
    padding: 0;
    display: block;
}

h4.toggle {
}

@media (min-width: 767px) {
    #menuDropDown {
        display: none;
    }
}

nav.topNav li.navItem {
    position: relative;
    text-align: center;
    display: -moz-inline-stack;
    display: inline-block;
    vertical-align: middle;
    zoom: 1;
}

.main-menu a:hover,.main-menu .current a {
    color: #7d27b0;
}

.content {
    padding: 30px 0;
}

@media (min-width: 1200px) {
    .content {
        max-width: 1200px;
        margin: 0 auto;
    }
}

.site-footer {
    text-align: center;
    font-size: 12px;
    margin-top: 50px;
    padding-bottom: 150px;
    background-color: #eaeaea;
}

@media (max-width: 767px) {
    .site-footer {
        padding-bottom: 50px;
    }
}

.site-footer ul {
    padding: 10px 0;
}

.site-footer li {
    display: inline-block;
    padding: 0;
}

.site-footer li a {
    padding: 0 20px;
    color: #989898;
}

/******* pagination* ******/
.pagination {
    float: right;
}

/*collections list */
.product.collectionsList {
    display: inline-block;
    width: 24%;
    margin-bottom: 60px;
}

@media (max-width: 767px) {
    .product.collectionsList {
        display: block;
        width: 100%;
    }
}
    /**** product-loop.liquid ****/
    .productTile {
        text-align: center;
        font-weight: 700;
        height: 300px;
    }

    .product.homepageBlocks {
        margin: 0;
    }

    .product-thumb {
        padding: 0;
    }

    .product-thumb img {
        max-width: 100%;
        height: auto;
    }

    .product-title {
    }

    .product-price {
        font-size: 14px;
    }

    /**** rte content  ****/
    .rte blockquote {
        font-style: italic;
        border-left: 3px solid #ddd;
        padding-left: 10px;
        margin-left: 0;
    }

    .rte td {
        border: 1px solid #ccc;
        padding: 5px;
    }

    /*lookbook*/
    .fullWidth {
        width: 100%;
    }

    /**** product.liquid ****/
    .flex-container {
        display: -webkit-flex;
        display: flex;
    }

    .alignitemsCenter {
        align-items: center;
    }

    .product-images.scroll {
        height: 100vh;
        overflow: scroll;
    }

    @media (max-width: 767px) {
    .product-images.scroll {
        height: initial;
    }
}

.product-scrolled-image {
    margin-right: 20px;
}

.product-zoomed-image. #zoomedImg {
    display: none;
}

.zoomedImg {
    display: none;
}

.zoomedImg.show {
    display: block;
    position: fixed;
    z-index: 999;
    width: 50%;
    text-align: center;
    top: 20%;
    left: 0;
}

.product-zoomed-image.show {
    display: block;
}

.product-details {
}

.product-images img {
    max-width: 100%;
    height: auto;
}

.product-header {
    margin-top: 0;
}

.product-desc,.price-field,.product-form {
    margin-bottom: 30px;
}

.price-field {
    font-weight: 700;
}

.selector-wrapper label,.selector-wrapper select {
    display: block;
}

.selector-wrapper label {
    float: left;
}

/*
.selector-wrapper select {
  float: right;
}*/
.product-variants {
    margin-bottom: 10px;
}

.product-form {
    text-align: right;
}

@media (max-width: 767px) {
    .product-form {
        text-align: left;
    }
}

select#product-select {
    border-radius: 10px;
    font: inherit;
    -webkit-appearance: menulist;
    margin: 10px 0;
}

/**** cart.liquid ****/
.cart-table {
    width: 100%;
}

.cart-table thead {
    border-bottom: 1px solid #ccc;
}

@media (max-width: 767px) {
    thead {
        display: none;
    }

    .cart-table td {
        display: block;
    }
}


.item-qty {
    width: 90px;
}

.cart-options {
    text-align: right;
}

.cart-info {
    line-height: 0;
}

/**** blog.liquid ****/
.blog-title {
    margin-bottom: 0;
}

.blog-article {
    padding-bottom: 30px;
    margin-bottom: 30px;
}

.blog-data {
    font-size: 13px;
    font-weight: 700;
    margin-top: 5px;
}

.blog-content {
    margin-bottom: 30px;
}

.blog-tags {
    width: 800px;
    font-weight: 700;
    font-size: 14px;
}

.blog-read-more {
    width: 122px;
}

.no-border {
    border: none !important;
}

/**** article.liquid ****/
.single-article {
    border-bottom: none;
    margin-bottom: 0;
}

/******** slider *********/
.rslides {
    position: relative;
    list-style: none;
    overflow: hidden;
    width: 100%;
    padding: 0;
    margin: 0;
}

.rslides li {
    -webkit-backface-visibility: hidden;
    position: absolute;
    display: none;
    width: 100%;
    left: 0;
    top: 0;
}

.rslides li:first-child {
    position: relative;
    display: block;
    float: left;
}

.rslides img {
    display: block;
    height: auto;
    float: left;
    width: 100%;
    border: 0;
}

